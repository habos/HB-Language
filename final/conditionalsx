function main(){
	#Define variables#
	var a = 10;
	var b = 5;
	var c = 2;

	#While loop#
	while(a > b){
		println(a);
		a = a - 2;
	}
	
	#if statements#
	if(c == 3){
		println("No");
	} else if(c == 2){
		println("Yes");
	} else {
		println("No");
	}

	#Nested conditionals#
	while(c == 2){
		if((b > c) & (a != b)){
			println("Inner Conditional");
		} else {
			println("No");
		}
		var d = 500;
		if((b == c) | (b < d)){
			println("Using or operator");
			c = 3;
		}
	}
}
main();
